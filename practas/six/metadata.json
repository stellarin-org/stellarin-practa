{
  "name": "Six",
  "version": "1.7.8",
  "description": "A daily word guessing game - guess the 6-letter word in 6 tries",
  "author": "Woodenfox",
  "estimatedDuration": 180,
  "dependencies": [
    "@expo/vector-icons",
    "expo-blur",
    "expo-haptics",
    "expo-sharing",
    "react-native-reanimated",
    "react-native-safe-area-context"
  ],
  "requiresAI": false,
  "configSchema": {
    "fields": {
      "aiEnabled": {
        "type": "boolean",
        "label": "AI Features",
        "description": "Enable AI-powered hints and suggestions",
        "default": true
      }
    }
  },
  "id": "six",
  "type": "widget",
  "category": "games",
  "tags": [
    "word",
    "puzzle",
    "daily"
  ],
  "requiredPermissions": [],
  "assets": {
    "splash": "splash.mp4",
    "icon": "six_icon.png",
    "sixLogo": "six_logo.png",
    "validWords": "valid-words.json",
    "wordlist": "wordlist.json",
    "hints": "hints.json"
  }
}